    <Color x:Key="ComboBoxOverColorColor">#454c5a</Color>
    <SolidColorBrush x:Key="ComboBoxTextColorSCB">#8fcaee</SolidColorBrush>
    <SolidColorBrush x:Key="ComboBoxBorderColorSCB">#8fcaee</SolidColorBrush>
    <Color x:Key="ComboBoxLeaveBorderColor">#c678cf</Color>
